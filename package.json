{
  "name": "set-children",
  "version": "0.0.2",
  "description": "reorder/replace DOM element children easily",
  "jsnext:main": "index.js",
  "main": "dist/set-children.js",
  "scripts": {
    "build": "npm run build-js && npm run uglify-js",
    "build-js": "rollup -n setChildren -f umd index.js -o dist/set-children.js",
    "uglify-js": "uglifyjs dist/set-children.js -cmo dist/set-children.min.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pakastin/set-children.git"
  },
  "author": "Juha Lindstedt",
  "license": "ISC",
  "devDependencies": {
    "rollup": "~0.34.7",
    "uglifyjs": "~2.4.10"
  }
}
